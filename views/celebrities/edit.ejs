<h1>Edit Celebrity</h1>

<form action="/celebrities/<%=celebrity._id%>" method="POST">
  <input type="text" name="name" value="<%= celebrity.name %>" placeholder="<%= celebrity.name %> ">
  <input type="text" name="occupation" value="<%= celebrity.occupation %>" placeholder="<%= celebrity.occupation %> ">
  <input type="text" name="catchPhrase" value="<%=celebrity.catchPhrase%>" placeholder="<%= celebrity.catchPhrase %> ">
  <input type="submit" value="submit">
</form>